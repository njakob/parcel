{
  "name": "@njakob/parcel",
  "version": "1.0.3",
  "description": "NPM package parser",
  "author": "Nicolas Jakob <nicow.jakob@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/njakob/parcel",
  "repository": {
    "type": "git",
    "url": "git://github.com/njakob/parcel.git"
  },
  "bugs": {
    "url": "https://github.com/njakob/parcel/issues"
  },
  "keywords": [
    "npm",
    "package",
    "parser",
    "helper"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build:flow": "flow gen-flow-files --out-dir lib src/index.js",
    "build:sources": "babel src -d lib --ignore '*.test.js'",
    "build": "npm run clean && npm run build:sources && npm run build:flow",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "flow": "flow check",
    "ava:watch": "ava -w",
    "ava": "ava",
    "test": "npm run lint && npm run flow && npm run ava",
    "release": "standard-version",
    "prepublish": "npm run test && npm run build"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.12.1",
    "eslint-config-njakob": "^4.1.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-flowtype": "^2.18.2",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.37.4",
    "rimraf": "^2.5.4",
    "standard-version": "^4.0.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
